# 【生产】环境变量

# 端口号（与开发环境保持一致）
VITE_PORT = 5173

# 网站地址前缀
VITE_BASE_URL = /

# API 请求基础路径（与开发环境保持一致，使用代理前缀）
VITE_API_URL = /api

# 生产环境代理的目标后端地址
# 网络访问时使用具体IP地址，确保其他设备可以访问
# 如果后端在同一台机器上，使用当前机器的IP地址
VITE_API_PROXY_URL = http://192.168.56.1:29578

# 备用API地址（当代理失败时使用）
VITE_FALLBACK_API_URL = http://192.168.56.1:29578

# Delete console（生产环境建议开启以优化性能）
VITE_DROP_CONSOLE = true